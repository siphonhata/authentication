# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Rate Limit Configuration
# Set to false to disable rate limiting entirely
SUPABASE_RATE_LIMIT_ENABLED=true
# Wait time in seconds before user can retry after hitting rate limit
SUPABASE_RATE_LIMIT_WAIT_TIME=60
# Custom error message (use {waitTime} as placeholder for wait time)
SUPABASE_RATE_LIMIT_MESSAGE=Too many OTP requests. Please wait {waitTime} seconds before trying again.
